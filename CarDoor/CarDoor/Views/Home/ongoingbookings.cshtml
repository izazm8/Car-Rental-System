<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>Dashio - Bootstrap Admin Template</title>

    <!-- Favicons -->
    <link href="img/favicon.png" rel="icon">
    <link href="img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Bootstrap core CSS -->
    <link href="~/Content/ap/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--external css-->
    <link href="~/Content/ap/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="~/Content/ap/css/style.css" rel="stylesheet">
    <link href="~/Content/ap/css/style-responsive.css" rel="stylesheet">

    <!-- =======================================================
      Template Name: Dashio
      Template URL: https://templatemag.com/dashio-bootstrap-admin-template/
      Author: TemplateMag.com
      License: https://templatemag.com/license/
    ======================================================= -->
</head>

<body style="margin-top:130px;">
    <section id="container">
        <!-- **********************************************************************************************************************************************************
            TOP BAR CONTENT & NOTIFICATIONS
            *********************************************************************************************************************************************************** -->
        <!--header start-->
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
            MAIN SIDEBAR MENU
            *********************************************************************************************************************************************************** -->
        <!-- **********************************************************************************************************************************************************
            MAIN CONTENT
            *********************************************************************************************************************************************************** -->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                <h3><i class="fa fa-angle-right"></i> Your Booking</h3>

                <!-- row -->
                <div class="row mt">
                    <div class="col-md-12">
                        <div class="content-panel">
                            <table class="table table-striped table-advance table-hover">
                                <h4><i class="fa fa-angle-right"></i> Bookings</h4>

                                <thead>
                                    <tr>
                                        <th><i class="fa fa-bullhorn"></i> ID</th>
                                        <th class="hidden-phone"><i class="fa fa-question-circle"></i> From Date</th>
                                        <th><i class="fa fa-bookmark"></i> To Date</th>
                                        <th><i class=" fa fa-edit"></i> Driver</th>
                                        <th><i class=" fa fa-edit"></i> Fine</th>
                                        <th><i class=" fa fa-edit"></i> Total</th>
                                        <th><i class=" fa fa-edit"></i> Status</th>
                                        <th><i class=" fa fa-edit"></i> MarkeAsCompleted</th>

                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var i in ViewBag.allbookings)
                                    {
                                        if (i.iscompleted == "no")
                                        {

                                            <tr>
                                                <td>
                                                    <a href="basic_table.html#">@i.Id</a>
                                                </td>
                                                <td class="hidden-phone">@i.pick_up_date</td>
                                                <td>@i.return_date </td>

                                                @if (i.driver == null)
                                                {
                                                    <td>NA</td>
                                                }
                                                @if (i.driver != null)
                                                {
                                                    <td>@i.driver</td>
                                                }

                                                @if (i.fine != null)
                                                {
                                                    <td>@i.fine</td>
                                                }

                                                @if (i.fine == null)
                                                {
                                                    <td>NA</td>
                                                }
                                                <td>@i.bill_with_fine</td>
                                                <td>@i.iscompleted</td>

                                                <td>

                                                    @{string link = "/Home/checkout?id=" + i.Id; }

                                                    <a href="@link" class="btn btn-success btn-xs"><i class="fa fa-check"></i></a>

                                                    @*@{string link = "/Home/disable_drivers?id=" + i.Id; }*@
                                                    @*@{string link_ = "/Home/edit_drivers?id=" + i.Id; }*@

                                                    @*<a href="#" class="btn btn-primary btn-xs"><i class="fa fa-pencil"></i></a>
        <a href="#" class="btn btn-danger btn-xs"><i class="fa fa-trash-o "></i></a>*@

                                                </td>
                                            </tr>
                                        }

                                    }


                                </tbody>
                            </table>
                        </div>
                        <!-- /content-panel -->
                    </div>
                    <!-- /col-md-12 -->
                </div>
                <!-- /row -->
            </section>
        </section>
        <!-- /MAIN CONTENT -->
        <!--main content end-->
        <!--footer start-->
        <!--footer end-->
    </section>
    <!-- js placed at the end of the document so the pages load faster -->
    <script src="~/Content/ap/lib/jquery/jquery.min.js"></script>
    <script src="~/Content/ap/lib/bootstrap/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="~/Content/ap/lib/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Content/ap/lib/jquery.scrollTo.min.js"></script>
    <script src="~/Content/ap/lib/jquery.nicescroll.js" type="text/javascript"></script>
    <!--common script for all pages-->
    <script src="~/Content/ap/lib/common-scripts.js"></script>
    <!--script for this page-->

</body>
</html>